{
  "name": "codeceptjs-docker",
  "version": "0.0.1",
  "description": "End to end tests for WordPress using CodeceptJS",
  "repository": {
    "type": "git",
    "url": "https://github.com/michaltrunek/codeceptjs-docker.git"
  },
  "scripts": {
    "test": "profile=normal codeceptjs run --steps",
